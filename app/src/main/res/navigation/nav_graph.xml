<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/login">
    <fragment
        android:id="@+id/login"
        android:name="fragments.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_home2"
            app:destination="@id/home2" />
        <action
            android:id="@+id/action_login_to_registerOptions2"
            app:destination="@id/registerOptions" />
    </fragment>
    <fragment
        android:id="@+id/home2"
        android:name="fragments.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/secondFragment"
        android:name="fragments.SecondFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/registerOptions"
        android:name="fragments.RegisterOptions"
        android:label="fragment_register_options"
        tools:layout="@layout/fragment_register_options" >
        <action
            android:id="@+id/action_registerOptions_to_secondFragment"
            app:destination="@id/secondFragment" />
    </fragment>
</navigation>